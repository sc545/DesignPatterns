package actionadventuregame;

public abstract class Character {
	WeaponBehavior weapon;
	abstract void fight();
	public Character(){
		
	}
	public void checkWeapon(){
		weapon.useWeapon();
	}
	public void setWeapon(WeaponBehavior wb){
		weapon = wb;
		System.out.print("무기 교체 : ");wb.useWeapon();
	}
}
